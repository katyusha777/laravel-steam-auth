{
    "name": "katyusha777/laravel-steam-auth",
    "description": "Laravel Steam Auth",
    "keywords": ["steam", "laravel", "auth"],
    "license": "MIT",
    "authors": [
        {
            "name": "Katyusha",
            "email": "noooooooooooooooooooo@no.com"
        }
    ],
    "require": {
        "php": "^8.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Invisnik\\LaravelSteamAuth\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Invisnik\\LaravelSteamAuth\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Invisnik\\LaravelSteamAuth\\SteamServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
